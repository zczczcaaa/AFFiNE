    "electron-window-state": "^5.0.3",
    "esbuild": "^0.24.0",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "lodash-es": "^4.17.21",
    "msw": "^2.6.8",
    "nanoid": "^5.0.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.28.0",
    "rxjs": "^7.8.1",
    "semver": "^7.6.3",
    "tree-kill": "^1.2.2",
    "ts-node": "^10.9.2",
    "uuid": "^11.0.3",
    "vitest": "2.1.8",
